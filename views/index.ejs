<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs') %> 
<body>

    <%- include('./partials/nav.ejs') %> 

    <div class="blogs content">
        <h2>All Blogs</h2>
        <% if (blogs.length>0) { %>
            <% blogs.forEach(blog => { %>
                <a href="/blogs/<%= blog._id %> ">
                <h3><%= blog.title %></h3>
                <p><%= blog.snippet %> </p>
                <p><%= blog.body %> </p>
            </a>
            <% }) %>
        <% } %>
    </div>

    <%- include('./partials/footer.ejs') %> 
</body>
</html>